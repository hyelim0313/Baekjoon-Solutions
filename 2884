#include <iostream>

    int main(){
        
        int H;
        int M;
        
        std::cin >> H;
        std::cin >> M;
        
        if(H > 0 && M < 45){
            std::cout << H-1 << " " << M + 15;
        } else if(H == 0 && M < 45){
            std::cout << 23 << " " << M + 15;
        } else if(H > 0 && M > 45){
            std::cout << H << " " << M - 45;
        } else{
            std::cout << H << " " << M - 45;
        }
    }
